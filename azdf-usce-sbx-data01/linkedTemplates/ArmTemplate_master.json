{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azdf-usce-sbx-data01"
		},
		"Azurestrgacctlinkedsvc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azurestrgacctlinkedsvc'"
		},
		"AzureBlobStorage1_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/2b87dee0-a31b-419f-94ab-192627b1321d/resourceGroups/rg-sbx-dataanalyticsvc/providers/Microsoft.Storage/storageAccounts/storuscesbxdata"
		},
		"AzureBlobStorage1_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azdf-usce-sbx-data01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azurestrgacctlinkedsvc_connectionString": {
						"value": "[parameters('Azurestrgacctlinkedsvc_connectionString')]"
					},
					"AzureBlobStorage1_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStorage1_properties_privateLinkResourceId')]"
					},
					"AzureBlobStorage1_properties_groupId": {
						"value": "[parameters('AzureBlobStorage1_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}